      "settings": {
        "image": "shopify:\/\/shop_images\/warehouse_to_customers_background_21-9_ab9d9e9a-a40f-439b-b41b-5d86ce67ed9f.jpg",
        "image_half": "shopify:\/\/shop_images\/warehouse_to_customers_background_21-9-half_a9031545-16d5-4207-9bc4-4bb5664c350e.jpg",
